{% extends 'PesFrontBundle::layout.html.twig' %}

{% block section %}
<div class="panel panel-default">
    <div class="panel-heading">Fiche Match</div>
    <div class="panel-body">

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Ajout</th>
                <td>{{ entity.ajout|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Resultat</th>
                <td>{{ entity.resultat }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('pes_competition') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pes_competition_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    </div>
        </div>
{% endblock %}

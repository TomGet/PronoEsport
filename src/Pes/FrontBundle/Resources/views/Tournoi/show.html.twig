{% extends 'PesFrontBundle::layout.html.twig' %}

{% block section %}
    <div class="panel panel-default">
        <div class="panel-heading">Fiche tournoi</div>
        <div class="panel-body">

            <table class="record_properties">
                <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ entity.id }}</td>
                </tr>
                <tr>
                    <th>Nom</th>
                    <td>{{ entity.nom }}</td>
                </tr>
                <tr>
                    <th>Site</th>
                    <td>{{ entity.site }}</td>
                </tr>
                </tbody>
            </table>

            <ul class="record_actions">
                <li>
                    <a href="{{ path('pes_tournoi_list') }}">
                        Back to the list
                    </a>
                </li>
                <li>
                    <a href="{{ path('pes_tournoi_edit', { 'id': entity.id }) }}">
                        Edit
                    </a>
                </li>
                <li>{{ form(delete_form) }}</li>
            </ul>
        </div>
    </div>
{% endblock %}
